{
  "name": "all-agents-tools",
  "version": "1.12.1",
  "type": "module",
  "scripts": {
    "build": "bun build --compile --outfile ../bin/aaa ./src/cli.ts",
    "check": "bun run lint && bun run typecheck && bun run test",
    "check:ci": "bun run format:check && bun run lint && bun run typecheck && bun run test",
    "dev": "bun run src/cli.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "cd .. && ./tools/node_modules/.bin/husky tools/.husky",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@commander-js/extra-typings": "^14.0.0",
    "@google/gemini-cli": "^0.16.0",
    "@octokit/rest": "^22.0.1",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "chokidar": "^5.0.0",
    "commander": "^14.0.0",
    "execa": "^9.6.1",
    "html-to-text": "^9.0.5",
    "marked": "^17.0.1",
    "marked-terminal": "^7.3.0",
    "ora": "^8.0.1",
    "parallel-web": "latest",
    "supports-hyperlinks": "^4.4.0",
    "wrap-ansi": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@types/html-to-text": "^9.0.4",
    "@types/node": "^24.10.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "bun-types": "latest",
    "eslint": "^9.39.1",
    "glob": "^13.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.3",
    "semantic-release": "^24.0.0",
    "typescript": "^5.9.3",
    "uba-eslint-config": "^1.3.0"
  }
}
