{
  "name": "all-agents-tools",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "bun build --compile --outfile ../bin/aaa ./src/cli.ts",
    "dev": "bun run src/cli.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "cd .. && ./tools/node_modules/.bin/husky tools/.husky",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@commander-js/extra-typings": "^14.0.0",
    "@google/gemini-cli": "^0.16.0",
    "@octokit/rest": "^22.0.1",
    "chalk": "^5.3.0",
    "commander": "^14.0.0",
    "dotenv": "^16.4.5",
    "execa": "^9.6.0",
    "ora": "^8.0.1",
    "parallel-web": "latest",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "bun-types": "latest",
    "eslint": "^9.39.1",
    "glob": "^13.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.3",
    "typescript": "^5.9.3",
    "uba-eslint-config": "^1.3.0"
  }
}
