#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/@google+gemini-cli@0.16.0_@opentelemetry+core@2.2.0_@opentelemetry+api@1.9.0__@opentele_ccb7350fe2fadf4e9ff3341309ac0c7c/node_modules/@google/gemini-cli/dist/node_modules:/workspace/node_modules/.pnpm/@google+gemini-cli@0.16.0_@opentelemetry+core@2.2.0_@opentelemetry+api@1.9.0__@opentele_ccb7350fe2fadf4e9ff3341309ac0c7c/node_modules/@google/gemini-cli/node_modules:/workspace/node_modules/.pnpm/@google+gemini-cli@0.16.0_@opentelemetry+core@2.2.0_@opentelemetry+api@1.9.0__@opentele_ccb7350fe2fadf4e9ff3341309ac0c7c/node_modules/@google/node_modules:/workspace/node_modules/.pnpm/@google+gemini-cli@0.16.0_@opentelemetry+core@2.2.0_@opentelemetry+api@1.9.0__@opentele_ccb7350fe2fadf4e9ff3341309ac0c7c/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/@google+gemini-cli@0.16.0_@opentelemetry+core@2.2.0_@opentelemetry+api@1.9.0__@opentele_ccb7350fe2fadf4e9ff3341309ac0c7c/node_modules/@google/gemini-cli/dist/node_modules:/workspace/node_modules/.pnpm/@google+gemini-cli@0.16.0_@opentelemetry+core@2.2.0_@opentelemetry+api@1.9.0__@opentele_ccb7350fe2fadf4e9ff3341309ac0c7c/node_modules/@google/gemini-cli/node_modules:/workspace/node_modules/.pnpm/@google+gemini-cli@0.16.0_@opentelemetry+core@2.2.0_@opentelemetry+api@1.9.0__@opentele_ccb7350fe2fadf4e9ff3341309ac0c7c/node_modules/@google/node_modules:/workspace/node_modules/.pnpm/@google+gemini-cli@0.16.0_@opentelemetry+core@2.2.0_@opentelemetry+api@1.9.0__@opentele_ccb7350fe2fadf4e9ff3341309ac0c7c/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
