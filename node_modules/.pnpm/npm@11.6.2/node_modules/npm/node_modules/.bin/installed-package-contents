#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspace/node_modules/.pnpm/npm@11.6.2/node_modules/npm/node_modules/@npmcli/installed-package-contents/bin/node_modules:/workspace/node_modules/.pnpm/npm@11.6.2/node_modules/npm/node_modules/@npmcli/installed-package-contents/node_modules:/workspace/node_modules/.pnpm/npm@11.6.2/node_modules/npm/node_modules/@npmcli/node_modules:/workspace/node_modules/.pnpm/npm@11.6.2/node_modules/npm/node_modules:/workspace/node_modules/.pnpm/npm@11.6.2/node_modules:/workspace/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspace/node_modules/.pnpm/npm@11.6.2/node_modules/npm/node_modules/@npmcli/installed-package-contents/bin/node_modules:/workspace/node_modules/.pnpm/npm@11.6.2/node_modules/npm/node_modules/@npmcli/installed-package-contents/node_modules:/workspace/node_modules/.pnpm/npm@11.6.2/node_modules/npm/node_modules/@npmcli/node_modules:/workspace/node_modules/.pnpm/npm@11.6.2/node_modules/npm/node_modules:/workspace/node_modules/.pnpm/npm@11.6.2/node_modules:/workspace/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@npmcli/installed-package-contents/bin/index.js" "$@"
else
  exec node  "$basedir/../@npmcli/installed-package-contents/bin/index.js" "$@"
fi
